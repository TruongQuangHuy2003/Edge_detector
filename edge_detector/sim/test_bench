#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55a72d39af60 .scope module, "test_bench" "test_bench" 2 2;
 .timescale -9 -12;
v0x55a72d3c2800_0 .var "clk", 0 0;
v0x55a72d3c28f0_0 .var "exp_pulse_out_n", 0 0;
v0x55a72d3c2990_0 .var "exp_pulse_out_p", 0 0;
v0x55a72d3c2a60_0 .net "pulse_out_n", 0 0, v0x55a72d3c1e60_0;  1 drivers
v0x55a72d3c2b30_0 .net "pulse_out_p", 0 0, v0x55a72d3c1f20_0;  1 drivers
v0x55a72d3c2c20_0 .var "rst_n", 0 0;
v0x55a72d3c2cf0_0 .var "sign_in", 0 0;
S_0x55a72d39b0f0 .scope module, "dut" "edge_detector" 2 12, 3 1 0, S_0x55a72d39af60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "sign_in";
    .port_info 3 /OUTPUT 1 "pulse_out_p";
    .port_info 4 /OUTPUT 1 "pulse_out_n";
v0x55a72d39b2f0_0 .net "clk", 0 0, v0x55a72d3c2800_0;  1 drivers
v0x55a72d3c1e60_0 .var "pulse_out_n", 0 0;
v0x55a72d3c1f20_0 .var "pulse_out_p", 0 0;
v0x55a72d3c1ff0_0 .net "rst_n", 0 0, v0x55a72d3c2c20_0;  1 drivers
v0x55a72d3c20b0_0 .net "sign_in", 0 0, v0x55a72d3c2cf0_0;  1 drivers
v0x55a72d3c21c0_0 .var "sign_in_prev", 0 0;
E_0x55a72d361060/0 .event anyedge, v0x55a72d3c1ff0_0;
E_0x55a72d361060/1 .event posedge, v0x55a72d39b2f0_0;
E_0x55a72d361060 .event/or E_0x55a72d361060/0, E_0x55a72d361060/1;
S_0x55a72d3c2320 .scope task, "verify" "verify" 2 25, 2 25 0, S_0x55a72d39af60;
 .timescale -9 -12;
v0x55a72d3c2520_0 .var "exp_pulse_out_n", 0 0;
v0x55a72d3c25e0_0 .var "exp_pulse_out_p", 0 0;
v0x55a72d3c26a0_0 .var "pulse_out_n", 0 0;
v0x55a72d3c2740_0 .var "pulse_out_p", 0 0;
TD_test_bench.verify ;
    %load/vec4 v0x55a72d3c25e0_0;
    %load/vec4 v0x55a72d3c2740_0;
    %cmp/e;
    %flag_get/vec4 4;
    %jmp/0 T_0.2, 4;
    %load/vec4 v0x55a72d3c2520_0;
    %load/vec4 v0x55a72d3c26a0_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_0.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %vpi_call 2 32 "$display", "-----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 33 "$display", "PASSED: Expected pulse_out_p: %b, Got pulse_out_p: %b, Expected pulse_out_n: %b, pulse_out_n: %b", v0x55a72d3c25e0_0, v0x55a72d3c2740_0, v0x55a72d3c2520_0, v0x55a72d3c26a0_0 {0 0 0};
    %vpi_call 2 34 "$display", "-----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 36 "$display", "-----------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 37 "$display", "PASSED: Expected pulse_out_p: %b, Got pulse_out_p: %b, Expected pulse_out_n: %b, pulse_out_n: %b", v0x55a72d3c25e0_0, v0x55a72d3c2740_0, v0x55a72d3c2520_0, v0x55a72d3c26a0_0 {0 0 0};
    %vpi_call 2 38 "$display", "-----------------------------------------------------------------------------------------------------------------" {0 0 0};
T_0.1 ;
    %end;
    .scope S_0x55a72d39b0f0;
T_1 ;
    %wait E_0x55a72d361060;
    %load/vec4 v0x55a72d3c1ff0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a72d3c21c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a72d3c1f20_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a72d3c1e60_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55a72d3c20b0_0;
    %load/vec4 v0x55a72d3c21c0_0;
    %inv;
    %and;
    %assign/vec4 v0x55a72d3c1f20_0, 0;
    %load/vec4 v0x55a72d3c20b0_0;
    %inv;
    %load/vec4 v0x55a72d3c21c0_0;
    %and;
    %assign/vec4 v0x55a72d3c1e60_0, 0;
    %load/vec4 v0x55a72d3c20b0_0;
    %assign/vec4 v0x55a72d3c21c0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55a72d39af60;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a72d3c2800_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x55a72d3c2800_0;
    %inv;
    %store/vec4 v0x55a72d3c2800_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x55a72d39af60;
T_3 ;
    %vpi_call 2 44 "$dumpfile", "test_bench.vcd" {0 0 0};
    %vpi_call 2 45 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55a72d39af60 {0 0 0};
    %vpi_call 2 47 "$display", "-------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 48 "$display", "------------------------------------TESTBENCH FOR EDGE DETECTOR ---------------------------------------------------" {0 0 0};
    %vpi_call 2 49 "$display", "-------------------------------------------------------------------------------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a72d3c2c20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a72d3c2cf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a72d3c2990_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a72d3c28f0_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x55a72d3c2990_0;
    %store/vec4 v0x55a72d3c25e0_0, 0, 1;
    %load/vec4 v0x55a72d3c2b30_0;
    %store/vec4 v0x55a72d3c2740_0, 0, 1;
    %load/vec4 v0x55a72d3c28f0_0;
    %store/vec4 v0x55a72d3c2520_0, 0, 1;
    %load/vec4 v0x55a72d3c2a60_0;
    %store/vec4 v0x55a72d3c26a0_0, 0, 1;
    %fork TD_test_bench.verify, S_0x55a72d3c2320;
    %join;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a72d3c2c20_0, 0, 1;
